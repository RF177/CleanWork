	<queryString>
		<![CDATA[select c.id_produto, D.DESCRICAO, sum(nvl(a.qtd, 0) * nvl(c.QTD, 0)) saida_producao
  from producao a
  join FICHATECNICA b ON b.id_produto = a.id_produto AND b.ID_FICHATECNICA = A.ID_FICHATECNICA
  join FICHATECNICA_INSUMO c ON c.ID_FICHATECNICA = b.ID_FICHATECNICA
  join CLEANWORK.produtos d ON d.id_produto = c.id_produto
 where a.SITUACAO = 3 AND a.data >= $P{dt1} AND a.data <= $P{dt2} $P!{filtroSql}
 group by c.id_produto, D.DESCRICAO]]>
	</queryString>
