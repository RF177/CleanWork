	<queryString>
		<![CDATA[SELECT a.id_venda, a.data_emissao, a.id_cliente, c.nome AS nome_cliente, a.valor_total,
CASE WHEN situacao = 3 THEN 'C' WHEN situacao = 2 THEN 'N' END as tipo_pedido
FROM vendas a

JOIN parceiros c ON c.id_parceiro = a.id_cliente

WHERE a.data_emissao >= $P{dt1} AND a.data_emissao <= $P{dt2}
$P!{filtroSql}

ORDER BY tipo_pedido DESC, a.data_emissao]]>
	</queryString>
